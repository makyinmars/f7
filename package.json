{
  "name": "f7-template",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "biome check && bun run lingui:compile && vite build && tsc --noEmit",
    "start": "bun .output/server/index.mjs",
    "check": "biome check",
    "check:all": "bun typecheck && biome check",
    "ci:check": "biome ci",
    "ci:format": "biome format --check",
    "ci:lint": "biome lint",
    "db:check": "bunx drizzle-kit check",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "bun ./src/db/migrate.ts",
    "db:pull": "drizzle-kit pull",
    "db:push": "drizzle-kit push --config=drizzle.config.ts",
    "db:seed": "bun ./src/db/seed/main-seed.ts",
    "db:studio": "drizzle-kit studio",
    "db:up": "drizzle-kit up",
    "format": "biome format --write ./src",
    "lint": "biome lint",
    "lint:fix": "biome check --write",
    "lint:unsafe-fix": "biome check --write --unsafe",
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "sso": "aws sso login --sso-session=engineering",
    "lingui:extract": "lingui extract --clean",
    "lingui:compile": "lingui compile"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@lingui/core": "^5.3.2",
    "@lingui/react": "^5.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@t3-oss/env-core": "^0.13.8",
    "@tailwindcss/vite": "^4.1.10",
    "@tanstack/react-query": "^5.80.7",
    "@tanstack/react-query-devtools": "^5.80.7",
    "@tanstack/react-router": "^1.121.16",
    "@tanstack/react-router-devtools": "^1.121.16",
    "@tanstack/react-router-with-query": "^1.121.16",
    "@tanstack/react-start": "^1.121.16",
    "@trpc/client": "^11.4.1",
    "@trpc/server": "^11.4.1",
    "@trpc/tanstack-react-query": "^11.4.1",
    "better-auth": "^1.2.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-orm": "^0.44.2",
    "drizzle-zod": "^0.8.2",
    "lucide-react": "^0.522.0",
    "motion": "^12.18.1",
    "next-themes": "^0.4.6",
    "pg": "^8.16.0",
    "radix-ui": "^1.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.58.0",
    "sonner": "^2.0.5",
    "sst": "^3.17.6",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.3.1",
    "vite": "^7.0.0",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0",
    "@lingui/babel-plugin-lingui-macro": "^5.3.2",
    "@lingui/cli": "^5.3.2",
    "@lingui/vite-plugin": "^5.3.2",
    "@types/aws-lambda": "8.10.150",
    "@types/node": "^22.5.4",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.31.1",
    "husky": "^9.1.7",
    "tailwindcss": "^4.1.10",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.7.2",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
